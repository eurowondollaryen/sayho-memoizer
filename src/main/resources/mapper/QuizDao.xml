  UPDATE ttpr001 
     SET username = #{username},
         age = #{age} 
   WHERE userid = #{userid}
  </update>
  
  <delete id="deleteUser" parameterType="int">
  DELETE FROM ttpr001
  WHERE userid = #{userid}
  </delete>
-->
</mapper>